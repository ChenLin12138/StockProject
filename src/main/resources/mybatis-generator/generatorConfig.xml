<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry
            location="/Users/zdm/.m2/repository/mysql/mysql-connector-java/8.0.15/mysql-connector-java-8.0.15.jar"/>
    <context id="my" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/stock" userId="root"
                        password="3306"/>

<!-- 请注意适当修改targetProject地址每次仅仅enable你想要修改的那个类，避免刷新了所有类-->
        <javaModelGenerator targetPackage="com.lin.stock.model"
                            targetProject="/Users/zdm/eclipse-workspace/StockProject/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.lin.stock.mappers"
                         targetProject="/Users/zdm/eclipse-workspace/StockProject/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        
        <javaClientGenerator targetPackage="com.lin.stock.mappers"
                             targetProject="/Users/zdm/eclipse-workspace/StockProject/src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>   
                
       <table tableName="PRICE_HISTORY" domainObjectName="PriceHistory"
       		enableCountByExample="false" enableUpdateByExample="false"
            enableDeleteByExample="false" enableSelectByExample="false"
            selectByExampleQueryId="false">   
       </table>  
    </context>
</generatorConfiguration>